<main>
  <%= form_tag "/applications" do %>
    <%= label_tag :pets %><br/>
    <%= select_tag :pets, options_from_collection_for_select(@pets, "id", "name"), multiple: true %><br/>
    <%= label_tag :name %><br/>
    <%= text_field_tag :name %><br/>
    <%= label_tag :address %><br/>
    <%= text_field_tag :address %><br/>
    <%= label_tag :city %><br/>
    <%= text_field_tag :city %><br/>
    <%= label_tag :state %><br/>
    <%= text_field_tag :state %><br/>
    <%= label_tag :zip %><br/>
    <%= text_field_tag :zip %><br/>
    <%= label_tag :phone %><br/>
    <%= text_field_tag :phone %><br/>
    <%= label_tag :description %><br/>
    <%= text_field_tag :description %><br/>
    <%= submit_tag "Submit Application" %>
  <% end %>
</main>